GUI { dial=Dial(max=physmax, maxheight=50,maxwidth=50) }
minval=20;
maxval=500;
physmax=maxval-minval
stdin.line(line) { dial.value = line; }
<Escape> || <Enter> || <C-g> { exit 0; }
function fib (n) {
  i = j = 1;
  m = 2;
  while (m++ <= n) {
    tmp = j;
    j = i + j;
    i = tmp;
  }
  return j;
}
dial.changed(from,to) { print dial, fib(to) }
<n> { dial.value+=1 }
<p> { dial.value-=1 }
